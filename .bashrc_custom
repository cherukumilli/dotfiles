if [ -e $HOME/ec2_custom/dotfiles/.ec2 ]; then
   source $HOME/ec2_custom/dotfiles/.ec2
fi

## The following are needed for Android development using Android Studio
export JAVA_HOME=/usr/lib/jvm/java-7-oracle
export JDK_HOME=/usr/lib/jvm/java-7-oracle
export ANDROID_SDK=$HOME/tools/android-studio/sdk
export PATH=$PATH:$HOME/google_appengine
export PATH=$PATH:$ANDROID_SDK/tools:$ANDROID_SDK/platform-tools